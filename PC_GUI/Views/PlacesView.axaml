<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PC_GUI.Views.PlacesView">
  <StackPanel Margin="10 0 10 10" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2" >
    <TextBlock> Places</TextBlock>
    
    <Button Command="{Binding BtnSubmitOnClickCommand}" Margin="10" Height="25" Width="100" HorizontalAlignment="Center">
      <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center">Add Place</TextBlock>
    </Button>
  </StackPanel>
</UserControl>
